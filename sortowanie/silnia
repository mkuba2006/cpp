long long int SilniaRek(short int n){
    int silnia=1, i;
    for(i=1;i<=n;i++){
        silnia= silnia *i;
    }
    return silnia;
}

int main()
{
    short int n;
    cout<<"n:";
    cin>>n;
    cout<<SilniaRek(n);
    return 0;
}
